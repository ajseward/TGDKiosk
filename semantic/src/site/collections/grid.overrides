/*******************************
         Site Overrides
*******************************/
.ui.squished.grid
{
	left:    0;
	padding: 0;
	margin:  0;
	> .column
	{
		padding: 0;
		margin:  0;
	}

	> .row > .column
	{
		padding: 0;
		margin:  0;
		> img
		{
			height:    auto;
			max-width: none !important;
			width:     100%;
		}
	}
}